julia = ("Julia", "Roberts", 1967, "Duplicity", 2009, "Actress", "Atlanta, Georgia")

print(julia[3:])
print(julia[4:])
print(julia[:3])
print(julia[:4])
print(julia[5:])


def f(r):
    c = 2 * math.pi * r
    a = math.pi * r * r
    return (c, a)


