mark = int(input("Enter the mark"))
if mark >= 75:
    print("Grade: First")
elif mark <75 and mark >=70:
    print("Grade: Upper Second")
elif mark <70 and mark >=60:
    print("Grade: Second")
elif mark <60 and mark >=50:
    print("Grade: Third")
elif mark <50 and mark >=45:
    print("Grade: F1 Supp")
elif mark <45 and mark >=40:
    print("Grade: F2")
else:
    print("Grade F3")

